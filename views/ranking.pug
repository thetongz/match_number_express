<!DOCTYPE>

html
    head
        title This is title
        link(href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet")
        script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js")
        link(rel="stylesheet" href="/css/style.css")
        style.
            body {
                font-family: 'Lato', sans-serif;
                text-align: left;
                padding-left: 50px
            }
            table {
                font-family: arial, sans-serif;
                border-collapse: collapse;
                width: 100%;
            }

            td, th {
                border: 1px solid #dddddd;
                text-align: left;
                padding: 8px;
            }

            th{
                background-color: #303F9F;
                color: white;
            }

            tr:nth-child(even) {
                background-color: #f2f2f2
            }

            a{
                text-decoration : none;
                color : black;
                font-size: 24px;
            }

    body

        div#easy
            h1 easy
            table(style="width:50%")
                tr
                    th name
                    th time
                    -for (var i = 0; i < data.easy.length ; i++)
                        tr
                            td #{data.easy[i].name}
                            td #{data.easy[i].time}
                            
        div#medium
            h1 medium
            table(style="width:50%")
                tr
                    th name
                    th time
                    -for (var i = 0; i < data.medium.length ; i++)
                        tr
                            td #{data.medium[i].name}
                            td #{data.medium[i].time}
        
        div#hard
            h1 hard
            table(style="width:50%")
                tr
                    th name
                    th time
                    -for (var i = 0; i < data.hard.length ; i++)
                        tr
                            td #{data.hard[i].name}
                            td #{data.hard[i].time}
